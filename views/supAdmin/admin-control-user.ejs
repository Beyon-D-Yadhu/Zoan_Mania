<%- include('../layouts/admin/header'); -%>
<div class="col-md-12">
                <table class="table table-striped table-bordered">
                    <thead>
                        <tr> 
                            <th class="thed" scope="col">No.</th>
                            <th class="thed" scope="col">Name</th>
                            <th class="thed" scope="col">Email</th>
                            <th class="thed" scope="col">Status</th>
                        </tr>
                    </thead>
                    <tbody>
                        <%userData.forEach((x)=>{ %>
                            <tr>
                                <td><i><%=++i%></i></td>
                                <td><i><%=x.name%></i></td>
                                <td><i><%=x.email%></i></td>
                                <td><i>Active</i></td>
                            </tr>
                        <%}) %>
                    </tbody>
                </table>
</div>
<%- include('../layouts/admin/footer'); -%>